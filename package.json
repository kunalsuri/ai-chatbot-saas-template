{
  "name": "saas-chatbot-ai",
  "version": "1.0.0",
  "description": "Modern SaaS ChatBot AI platform with advanced conversation capabilities. See AI-PROJECT-MANIFEST.md for complete project context.",
  "type": "module",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:local": "NODE_ENV=development HOST=localhost tsx server/index.ts",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "node scripts/build-server.js",
    "setup:local": "chmod +x scripts/setup-local.sh && ./scripts/setup-local.sh",
    "validate:manifest": "node scripts/validate-manifest.js",
    "sync:manifests": "node scripts/sync-manifests.js",
    "type-check": "tsc --noEmit --project tsconfig.dev.json",
    "type-check:strict": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.cache",
    "deps:update": "npm update && npm audit fix",
    "security:audit": "npm audit --audit-level moderate",
    "test:all": "chmod +x scripts/run-all-tests.sh && ./scripts/run-all-tests.sh",
    "ts:summary": "chmod +x scripts/typescript-summary.sh && ./scripts/typescript-summary.sh"
  },
